{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "naive_bayes": {
        "type": "object",
        "properties": {
            "discretization_bins": { "type": "integer", "minimum": 2 },
            "smoothing_alpha": { "type": "number", "exclusiveMinimum": 0 }
        }
    },
    "xgboost": {
        "type": "object",
        "properties": {
            "hyperparameter_tuning": {
                "type": "object",
                "properties": {
                    "method": { "type": "string" },
                    "n_trials": { "type": "integer", "minimum": 1 }
                }
            },
            "default_params": {
                "type": "object",
                "properties": {
                    "max_depth": { "type": "integer" },
                    "learning_rate": { "type": "number" },
                    "n_estimators": { "type": "integer" }
                }
            }
        }
    },
    "lstm": {
        "type": "object"
    },
    "mitra": {
        "type": "object"
    }
  }
}
